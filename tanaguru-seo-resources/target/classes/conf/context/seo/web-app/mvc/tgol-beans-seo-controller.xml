<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

     <!-- Add seo elements to the audit form field builder  --> 
    <bean class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
        <property name="targetObject">
            <ref bean="auditResultController"/>
        </property>
        <property name="targetMethod">
            <value>setFormFieldBuilderList</value>
        </property>
        <property name="arguments">
             <list>
                <ref bean="aw21ThemeSelectFormFieldBuilder"/>
                <ref bean="aw22ThemeSelectFormFieldBuilder"/>
                <ref bean="seoThemeSelectFormFieldBuilder"/>
                <ref bean="rgaa22ThemeSelectFormFieldBuilder"/>
                <ref bean="testResultCheckboxFormFieldBuilder"/>
            </list>
        </property>
    </bean>
    
</beans>